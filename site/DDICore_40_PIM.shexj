    {
      "id": "http://ddi-alliance.org/ns/#LogicalRecord",
      "type": "ShapeDecl",
      "abstract": false,
      "shapeExpr": {
        "type": "Shape",
        "closed": true,
        "expression": {
          "type": "TripleConstraint",
          "predicate": "http://ddi-alliance.org/ns/#isDefinedBy",
          "valueExpr": "http://ddi-alliance.org/ns/#Concept",
