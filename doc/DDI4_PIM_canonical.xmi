					<ownedAttribute xmi:id="DDI4_PIM-ClassLibrary-Workflows-MetadataDrivenAction-quasiVTL" xmi:uuid="http://ddialliance.org/Specification/DDI-Lifecycle/4.0/DDI4_PIM.xmi#DDI4_PIM-ClassLibrary-Workflows-MetadataDrivenAction-quasiVTL" xmi:type="uml:Property">
