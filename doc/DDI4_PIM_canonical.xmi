						<upperValue xmi:id="DDI4_PIM-ClassLibrary-Conceptual-InstanceVariable-platformType-upperValue" xmi:uuid="http://ddialliance.org/Specification/DDI-Lifecycle/4.0/DDI4_PIM.xmi#DDI4_PIM-ClassLibrary-Conceptual-InstanceVariable-platformType-upperValue" xmi:type="uml:LiteralInteger">
							<value>1</value>
						</upperValue>
